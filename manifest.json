{
    "manifest_version": 2,
    "version": "0.1",
    "name": "Bookmark Desktop",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "topSites",
        "bookmarks",
        "tabs",
        "chrome://favicon/"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "minimum_chrome_version": "59"
}
